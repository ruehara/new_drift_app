    "version": "0.2.0",
    "configurations": [
        {
          "name": "Launch Develop",
          "request": "launch",
          "type": "dart",
          "program": "lib/main.dart",
          "args": [
            "--target", "lib/main.dart", 
            "--dart-define", "app_flavor=development",
            "--dart-define", "BASE_URL=https://app.dev.example.com",
            "--dart-define", "API_KEY=12345"
          ],
        },
        {
          "name": "Launch Staging",
          "request": "launch",
          "type": "dart",
          "program": "lib/main.dart",
          "args": [
            "--target", "lib/main.dart", 
            "--dart-define", "app_flavor=staging",
            "--dart-define", "BASE_URL=https://app.qa.example.com",
            "--dart-define", "API_KEY=12345"
          ],
        },
        {
          "name": "Launch Production",
          "request": "launch",
          "type": "dart",
          "program": "lib/main.dart",
          "args": [
            "--target", "lib/main.dart", 
            "--dart-define", "app_flavor=production",
            "--dart-define", "BASE_URL=https://app.example.com",
            "--dart-define", "API_KEY=12345"
          ],
            
        }
      ]
  }